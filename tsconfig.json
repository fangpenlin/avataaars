{
    "compilerOptions": {
      "target": "es2015",
      "module": "es2015",
      "moduleResolution": "node",
      "jsx": "react",
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "strictNullChecks": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "declaration": true,
      "outDir": "./dist"
    },
    "types": ["jest", "node"]
  }
  